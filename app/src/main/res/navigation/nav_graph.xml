<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/inputFragment">

    <fragment
        android:id="@+id/inputFragment"
        android:name="kz.app.assignment3.exercise2.FragmentInput"
        tools:layout="@layout/fragment_input">
        <action
            android:id="@+id/action_inputFragment_to_outputFragment"
            app:destination="@id/outputFragment" />
    </fragment>

    <fragment
        android:id="@+id/outputFragment"
        android:name="kz.app.assignment3.exercise2.FragmentOutput"
        tools:layout="@layout/fragment_output" />
</navigation>